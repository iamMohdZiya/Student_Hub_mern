# Docker Environment Variables for StudentHub
NODE_ENV=production
DOCKER=true
PORT=3000

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/studenthub

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here_replace_with_secure_key

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Upload Configuration
MAX_FILE_SIZE=5MB

# Session Configuration
SESSION_SECRET=your_session_secret_here_replace_with_secure_key
